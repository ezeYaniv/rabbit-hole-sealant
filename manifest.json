{
  "name": "Rabbit Hole Sealant",
  "description": "Stop falling into YouTube rabbit holes!",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/*"
      ],
      "js": [
        "jquery-3.6.0.min.js",
        "removeRelated.js"
      ]
    }
  ]
}
